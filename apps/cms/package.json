{
  "name": "cms",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "copy-schemas": "for d in src/api/*/; do name=$(basename \"$d\"); if [ -d \"src/api/$name/content-types\" ]; then mkdir -p \"dist/src/api/$name/content-types\"; cp -r \"src/api/$name/content-types/\"* \"dist/src/api/$name/content-types/\"; fi; done",
    "build": "strapi build && npm run copy-schemas",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "npm run copy-schemas && strapi develop",
    "develop": "npm run copy-schemas && strapi develop",
    "start": "npm run copy-schemas && strapi start",
    "strapi": "strapi",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.33.4",
    "@strapi/plugin-users-permissions": "5.33.4",
    "@strapi/strapi": "5.33.4",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "12.4.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "sqlite3": "^5.1.7",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0 <=24.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "0c8744c3-475f-4f46-b702-3d07c4b172a2",
    "installId": "6046c425e89fc0fb21903e307dd5ba3fdf0a3282459de9318ce45c464790c754"
  }
}
